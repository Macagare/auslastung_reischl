<p id="notice"><%= notice %></p>

<h1>Listing Timetables</h1>

<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Slot1</th>
      <th>Slot2</th>
      <th>Slot3</th>
      <th>Slot4</th>
      <th>Employee</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @timetables.each do |timetable| %>
      <tr>
        <td><%= timetable.date %></td>
        <td><%= timetable.slot1 %></td>
        <td><%= timetable.slot2 %></td>
        <td><%= timetable.slot3 %></td>
        <td><%= timetable.slot4 %></td>
        <td><%= timetable.employee %></td>
      </tr>
    <% end %>
  </tbody>
</table>

<br>

<%= link_to 'New Timetable', new_employee_timetable_path %>
