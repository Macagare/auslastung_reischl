<table class="table">
  <thead>
    <tr>
      <th>Date</th>
      <th>Slot 1</th>
      <th>Slot 2</th>
      <th>Slot 3</th>
      <th>Slot 4</th>
    </tr>
  </thead>
  <tbody>
    <% employee.timetables.limit(10).each do |timetable| %>
    <tr>
      <td><%= timetable.date %></td>
      <%= render 'layouts/slot', available: timetable.slot1 %>
      <%= render 'layouts/slot', available: timetable.slot2 %>
      <%= render 'layouts/slot', available: timetable.slot3 %>
      <%= render 'layouts/slot', available: timetable.slot4 %>
    </tr>
    <% end %>
  </tbody>

</table>
