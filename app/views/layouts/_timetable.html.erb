<table>
  <thead>
    <tr>
      <th>Date</th>
      <th>Slot 1</th>
      <th>Slot 2</th>
      <th>Slot 3</th>
      <th>Slot 4</th>
    </tr>
  </thead>
  <tbody>
    <% employee.timetables.each do |timetable| %>
    <tr>
      <td><%= timetable.date %></td>
      <td><%= timetable.slot1 %></td>
      <td><%= timetable.slot2 %></td>
      <td><%= timetable.slot3 %></td>
      <td><%= timetable.slot4 %></td>
    </tr>
    <% end %>
  </tbody>

</table>
