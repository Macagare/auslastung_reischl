<h1>Listing Employees</h1>
<%= link_to 'Need Employee', book_employees_path %>
<table class="table">
  <thead>
    <tr>
      <th>Name</th>
      <th colspan="3"></th>
    </tr>
  </thead>

  <tbody>
    <% @employees.each do |employee| %>
      <tr>
        <td><%= employee.name %></td>
        <td><%= render 'layouts/timetable', employee: employee %></dt>
      </tr>
    <% end %>
  </tbody>
</table>

<br>
